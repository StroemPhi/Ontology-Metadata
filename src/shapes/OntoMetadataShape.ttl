        sh:path <http://omv.ontoware.org/2005/05/ontology#hasOntologySyntax>;
        sh:severity sh:Warning ;
        sh:minCount 1 ;
        sh:message "The ontology does not specify its applied syntax. The value should be provided via IRI from the following controlled vocabulary: https://www.w3.org/ns/formats/."@en ;
        sh:description "The ontology should specify its applied syntax.  The value should be provided via IRI from the following controlled vocabulary: https://www.w3.org/ns/formats/."@en ;
