## KOS type
### https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.t43hvhmk2o1b

    sh:property [
        sh:path dcterms:type ;
        sh:severity sh:Violation ;
        # sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
        sh:message "The ontology does not declare its KOS type via identifier from the NKOS type vocabulary  (https://nkos.dublincore.org/nkostype/nkostype.rdf)."@en ;
        sh:description "If the ontology declares a KOS type, it must do so by using an identifier from the NKOS type vocabulary (https://nkos.dublincore.org/nkostype/nkostype.rdf)."@en ;
        sh:name "KOS type"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path dcterms:type ;
        sh:severity sh:Violation ;
        # sh:pattern "http://w3id.org/nkos/nkostype#[a-z]+" ;
        sh:xone (
                [sh:hasValue <http://w3id.org/nkos/nkostype#authority_file>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#categorization_schema>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#classification_schema>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#dictionary>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#gazetteer>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#glossary>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#list>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#name_authority_list>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#ontology>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#pick_list>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#semantic_network>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#subject_heading_scheme>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#synonym_ring>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#taxonomy>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#terminology>]
                [sh:hasValue <http://w3id.org/nkos/nkostype#thesaurus>]
            ) ;
        sh:message "The ontology's KOS type identifier is not from the NKOS type vocabulary  (https://nkos.dublincore.org/nkostype/nkostype.rdf)."@en ;
        sh:description "If the ontology declares a KOS type, the provided IRI must be from the NKOS type vocabulary namespace (https://nkos.dublincore.org/nkostype/nkostype.rdf)."@en ;
        sh:name "KOS type"@en ; # this if for forms with DASH
    ] ;

