@base <http://www.purl.org/OntoMeta> .
@prefix ontom:    <http://www.purl.org/OntoMeta#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix schema: <http://schema.org/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix pav: <http://purl.org/pav/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix premis: <http://www.loc.gov/premis/rdf/v3/> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .

# I import SHACL so that we can look stuff up quickly locally
[rdf:type owl:Ontology ; owl:imports <http://www.w3.org/ns/shacl#> ] .

# Start of the Ontology Metadata Shape
ontom:OntologyMetadataShape a sh:NodeShape ;
    sh:targetClass owl:Ontology ; 
# MANDATORY METADATA
## G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.w5pmqfs8xiuv 

## Ontology title
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.1exx8hu4osgx
    sh:property [              
        sh:path [sh:alternativePath (rdfs:label skos:prefLabel dc:title dcterms:title schema:name) ] ; 
        sh:severity sh:Violation ;
        sh:minCount 1 ;
        sh:message "The ontology does not specify its label or title."@en ;
        sh:description "The ontology must specify its label or title."@en ;
        sh:name "Title"@en ; # this if for forms with DASH
        sh:order 1 ; # also for forms, order of fields
    ] ;

    sh:property [             
        sh:path [sh:alternativePath (rdfs:label skos:prefLabel dc:title dcterms:title schema:name) ] ; 
        sh:severity sh:Info ;
        sh:datatype rdf:langString ;
        sh:message "The ontology title is not an rdf:langString, i.e. it does not carry a language tag. We recommend to add one."@en ;
        sh:description "This shape checks whether the ontology title literal value is an rdf:langString. It will produce an info message if the title does not carry a language tag."@en ;
        sh:name "Title"@en ; # this if for forms with DASH
        sh:order 1 ; # also for forms, order of fields
    ] ;

## Preferred prefix/ ontology acronym
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.us2ezyzaf4h4
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.1iqmwa13y7u
    sh:or (
        [
            sh:property [
            sh:path <http://purl.org/vocab/vann/preferredNamespacePrefix> ;
            sh:severity sh:Warning;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:message "The ontology does not declare its preferred namespace prefix. The ontology should provide its preferred prefix or an acronym."@en ;
            sh:description "The ontology does not declare its preferred namespace prefix. The ontology should provide its preferred prefix or an acronym."@en ;
            sh:name "Preferred prefix"@en ; # this if for forms with DASH
            sh:description "The ontology must declare its preferred namespace prefix."@en ;
            ] ;
        ]
        [
            sh:property [
            sh:path <https://w3id.org/mod#acronym> ;
            sh:severity sh:Warning;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:message "The ontology does not declare an acronym. The ontology should provide an acronym or its preferred prefix."@en ;
            sh:description "The ontology must declare an acronym. The ontology should provide an acronym or its preferred prefix."@en ;
            sh:name "Acronym"@en ; # this if for forms with DASH
            ] ;
        ]
    );

    sh:property [
        sh:path <http://purl.org/vocab/vann/preferredNamespacePrefix> ;
        sh:severity sh:Warning ;
        sh:datatype xsd:string ;
        sh:message "The ontology's preferred namespace prefix is not an xsd:string."@en ;
        sh:description "The ontology's preferred namespace prefix should be an xsd:string."@en ;
        sh:name "Preferred prefix"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path <https://w3id.org/mod#acronym> ;
        sh:severity sh:Violation ;
        sh:datatype xsd:string ;
        sh:message "The ontology's acronym is not an xsd:string."@en ;
        sh:description "The ontology's acronym should be an xsd:string."@en ;
        sh:name "Acronym"@en ; # this if for forms with DASH
    ] ;

## Ontology license
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.d63b5oqb03v
### Der TS nimmt die Lizenz-IRI, aber gern auch ein Label - darüber hatten wir noch nicht gesprochen
    sh:property [
        sh:path [sh:alternativePath (dcterms:license dcterms:rights schema:license cc:license dbo:license)] ;
        sh:severity sh:Violation ;
        sh:minCount 1;
        # tbd/todo: specify a list of accepted licenses? e.g. from open definition
        sh:nodeKind sh:IRI ;
        sh:message "The ontology does not specify any license or it does not provide it as an IRI. Please check, whether you have added a license statement and that it is not a literal or blank node. We also recommend to use an open license. Read more about open licenses at https://opendefinition.org/licenses/."@en ;
        sh:description "This shape checks whether the ontology provides license information. The license should be provided as an IRI (https://www.w3.org/TR/rdf11-concepts/#dfn-iri), not as a Literal. The license should be open (see more at https://opendefinition.org/licenses/)."@en ;
        sh:name "License"@en ;
    ] ;

## Ontology creator
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.uc5cfjvqg7cv
### Über regex-Prüfungen könnte man hier auch bestimmte Identifier enforcen
    sh:property [
        sh:path  [sh:alternativePath (dc:creator dcterms:creator schema:creator pav:createdBy prov:wasAttributedTo)];
        sh:severity sh:Violation ;
        sh:minCount 1;
        sh:message "The ontology does not state its creator. We recommend to provide PIDs for individuals (e.g. ORCiD) or organisations (e.g. ROR)."@en ;
        sh:description "The ontology must state its creator. We recommend to provide PIDs for individuals (e.g. ORCiD) or organisations (e.g. ROR)."@en ;
        sh:name "Creator(s)"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path  [sh:alternativePath (dc:creator dcterms:creator schema:creator pav:createdBy prov:wasAttributedTo)];
        sh:severity sh:Info ;
        sh:nodeKind sh:IRI;
        sh:message "The ontology does not state its creator via IRI. We recommend to provide PIDs for individuals (e.g. ORCiD) or organisations (e.g. ROR)."@en ;
        sh:description "The ontology should state its creator via IRI. We recommend to provide PIDs for individuals (e.g. ORCiD) or organisations (e.g. ROR)."@en ;
        sh:name "Creator(s)"@en ;
    ] ;

## Version information
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.88o633ocogle

### Version IRI
#### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.5o62yvmk2eo1
    sh:property [
        sh:path  owl:versionIRI ;
        sh:severity sh:Info ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "The ontology does not state exactly one versionIRI. The versionIRI should be stable and resolvable, i.e. a fixed version of the ontology should be retrievable from the provided IRI."@en ;
        sh:description "This shape checks whether the ontology provids exactly one versionIRI. The versionIRI should be stable and resolvable, i.e. a fixed version of the ontology should be retrievable from the provided IRI."@en ;
        sh:name "Version IRI"@en ;
    ] ;

    sh:property [
        sh:path  owl:versionIRI ;
        sh:severity sh:Info ;
        sh:nodeKind sh:IRI;
        sh:message "The ontology does not state the versionIRI as an IRI. The versionIRI should be stable and resolvable, i.e. a fixed version of the ontology should be retrievable from the provided IRI."@en ;
        sh:description "This shape checks the ontology versionIRI. The ontology versionIRI must be an IRI (https://www.w3.org/TR/rdf11-concepts/#dfn-iri), not a Literal. The versionIRI should be stable and resolvable, i.e. a fixed version of the ontology should be retrievable from the provided IRI."@en ;
        sh:name "Version IRI"@en ;
    ] ;

### Creation date
#### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.7i4grrp5qlng
    sh:property [
        sh:path [sh:alternativePath (dcterms:created schema:dateCreated pav:createdOn prov:generatedAtTime )] ;
        sh:severity sh:Violation ;
        sh:minCount 1;
        sh:maxCount 1 ;
        sh:message "The ontology does not specify exactly one date of creation. The date must be given in xsd:dateTime format."@en ;
        sh:description "The ontology must specify exactly one date of creation in xsd:dateTime format."@en ;
        sh:name "Creation date"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (dcterms:created schema:dateCreated pav:createdOn prov:generatedAtTime )] ;
        sh:severity sh:Violation ;
        sh:datatype xsd:dateTime ;
        sh:message "The ontology does not specify its date of creation in xsd:dateTime format."@en ; 
        sh:description "The ontology must specify its date of creation in xsd:dateTime format."@en ;
        sh:name "Creation date"@en ; # this if for forms with DASH
    ] ;

### Modification date
#### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.i101l8f9ynz8
    sh:property [
        sh:path [sh:alternativePath (dcterms:modified schema:dateModified pav:curatedOn)];
