        sh:message "The ontology does not specify at least one modification date in xsd:dateTime format."@en ;
        sh:description "The ontology must specify at least one modification date in xsd:dateTime format."@en ;
        sh:name "Modification date"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (dcterms:modified schema:dateModified pav:curatedOn)];
        sh:severity sh:Violation ;
        sh:datatype xsd:dateTime ;
        sh:message "The ontology does not specify its modification date in xsd:dateTime format."@en ;
        sh:description "The ontology must specify its modification date(s) in xsd:dateTime format."@en ;
        sh:name "Modification date"@en ; # this if for forms with DASH
    ] ;

### Textual version information
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.i101l8f9ynz8
    sh:property [
        sh:path [sh:alternativePath (owl:versionInfo pav:version)];
        sh:severity sh:Violation ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "The ontology does not specify exactly one information about its version. We recommend semantic versioning(?) or latest modification/relase date (?)."@en ; # tbd
        sh:description "The ontology must(?) specify exactly one information about its version. We recommend semantic versioning(?) or latest modification/relase date (?)."@en ; #tbd
        sh:name "Version information"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (owl:versionInfo pav:version)];
        sh:severity sh:Violation ;
        sh:datatype xsd:string ;
        sh:message "The ontology does not specify its version information as xsd:string. We recommend semantic versioning(?) or latest modification/relase date (?)."@en ; # tbd
        sh:description "The ontology must(?) specify information about its version as xsd:string. We recommend semantic versioning(?) or latest modification/relase date (?)."@en ; #tbd
        sh:name "Version information"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (owl:versionInfo pav:version)];
        sh:severity sh:Violation ;
        sh:xone (
            [sh:pattern "^[0-9].[0-9].[0-9]$" ;]
            [sh:pattern "^[0-9]{4}-[0-9]{2}-[0-9]{2}$" ;]
        ) ;
        sh:message "The ontology version information does not match the semantic versioning (e.g. 1.2.6) or date versioning pattern (e.g. 2023-11-12)."@en ; # tbd
        sh:description "The ontology version information must match the semantic versioning (e.g. 1.2.6) or date versioning pattern (e.g. 2023-11-12)."@en ; #tbd
        sh:name "Version information"@en ; # this if for forms with DASH
    ] ;

### Version notes
