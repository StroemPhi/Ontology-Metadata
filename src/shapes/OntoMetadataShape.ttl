        sh:description "The ontology must(?) give exactly one update description (e.g. a comparison between this version and the directly preceeding version)."@en ; # tbd
        sh:name "Version notes"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (adms:versionNotes vann:changes)]  ;
        sh:severity sh:Violation ;
        sh:nodeKind sh:Literal ; 
        sh:datatype rdf:langString ;
        sh:message "The ontology does not provide its version notes as an rdf:langString."@en ; # tbd
        sh:description "The ontology must (?) provide its version notes as an rdf:langString."@en ; # tbd
        sh:name "Version notes"@en ; # this if for forms with DASH
    ] ;

## Ontology description
### G-Doc https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.bfn4i6l0oqot
    sh:property [
        sh:path [sh:alternativePath (dc:description dcterms:description schema:description)] ;
        sh:severity sh:Violation ;
        sh:minCount 1 ;
        sh:message "The ontology does not provide at least one short description about its scope."@en ;
        sh:description "The ontology must provide at least one short description about its scope. The description should be no more 500 characters long. There must be at most one description per language."@en ;
        sh:name "Description"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (dc:description dcterms:description schema:description)] ;
        sh:severity sh:Violation ;
        sh:uniqueLang true ;
        sh:message "The ontology provides more than one short descriptions about its scope in the same language."@en ;
        sh:description "The ontology must provide at least one short description about its scope. The description should be no more 500 characters long. There must be at most one description per language."@en ;
        sh:name "Description"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (dc:description dcterms:description schema:description)] ;
        sh:severity sh:Violation ;
        sh:maxLength 500 ; #tbd Wir haben Ontologien mit sehr viel längeren Beschreibungen... Aber das könnte man auch in ein ausführliches Abstract schieben.
        sh:message "The ontology description is too long (500 characters)."@en ;
        sh:description "The ontology must provide at least one short description about its scope. The description should be no more 500 characters long. There must be at most one description per language."@en ;
        sh:name "Description"@en ; # this if for forms with DASH
    ] ;

    sh:property [
        sh:path [sh:alternativePath (dc:description dcterms:description schema:description)] ;
        sh:severity sh:Violation ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:message "The ontology does not provide the short description about its scope as an rdf:langString."@en ;
        sh:description "The ontology must provide at least one short description about its scope. The description should be no more 500 characters long. There must be at most one description per language."@en ;
        sh:name "Description"@en ; # this if for forms with DASH
    ] ;

