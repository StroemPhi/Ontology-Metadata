## Format/ media type/ content type
### https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.38comxqwn2em

    sh:property [
        sh:path [sh:alternativePath (<http://purl.org/dc/terms/format> <http://purl.org/dc/elements/1.1/format>)] ;
        sh:severity sh:Violation ;
        sh:xone (
            # for the TS rdf+xml and turtle are confirmed, we did not test the others yet
            [sh:hasValue <https://www.iana.org/assignments/media-types/application/rdf+xml> ;] 
            [sh:hasValue <https://www.iana.org/assignments/media-types/text/turtle> ;]
            [sh:hasValue <https://www.iana.org/assignments/media-types/application/trig> ;]
            [sh:hasValue <https://www.iana.org/assignments/media-types/text/n3> ;]
            [sh:hasValue <https://www.iana.org/assignments/media-types/application/n-triples> ;]
            [sh:hasValue <https://www.iana.org/assignments/media-types/application/n-quads> ;]
            [sh:hasValue <https://www.iana.org/assignments/media-types/application/ld+json> ;]
            # [sh:hasValue <> ;]
            # [sh:hasValue <> ;]
        );
        sh:message "The media type of the ontology is not one of the allowed values from the Media Types list of the Internet Assigned Number Authority (IANA) (cf. https://www.iana.org/assignments/media-types/media-types.xhtml)"@en ;
        sh:description "The media type of the ontology must be one of the allowed values from the Media Types list of the Internet Assigned Number Authority (IANA) (cf. https://www.iana.org/assignments/media-types/media-types.xhtml)."@en ;
        sh:name "Media type"@en ;
    ] ;

