## Publisher
### https://docs.google.com/document/d/1FRmCQ7eD6PcqSQgPbqCGjki3t39ROoYozsehCPX9E0Q/edit#bookmark=id.wjrgmpmdrs36
# tbd > Test schreibt keine maximale Anzahl an Publishern vor, und triggert sobald ein Wert nicht dem pattern entspricht - selbst dann, wenn der Ontology Editor bereits drei dem Pattern entsprechende Publisher-IDs nennt
    sh:property [
        sh:path [sh:alternativePath (<http://purl.org/dc/terms/publisher> <http://purl.org/dc/elements/1.1/publisher> <http://schema.org/publisher>)] ;
        sh:severity sh:Violation ;
        sh:nodeKind sh:IRI ;
        sh:xone (
            # pattern sources: Wikidata entries of the identifier types
            [sh:pattern "https://ror.org/([a-z0-9]{9})" ; ]
            [sh:pattern "https://isni.org/isni/[0]{4}[0-9]{4}[0-9]{4}[0-9]{3}[0-9X]" ;]
            [sh:pattern "https://d-nb.info/gnd/(|(1[012]?[0-9]{7}[0-9X]|[47][0-9]{6}-[0-9]|[1-9][0-9]{0,7}-[0-9X]|3[0-9]{7}[0-9X]))$" ;]
        ) ;
        sh:message "The ontology does not specify its publisher as an IRI belonging to the ROR, ISNI or GND namespace."@en ;
        sh:description "The ontology must specify its publisher as an IRI belonging to the ROR, ISNI or GND namespace."@en ; #tbd
        sh:name "Publisher"@en ;
    ] ;

