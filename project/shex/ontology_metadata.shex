BASE <hhttp://terminology.tib.eu/schema/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX linkml: <https://w3id.org/linkml/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dc: <http://purl.org/dc/terms/>
PREFIX idot: <http://identifiers.org/>
PREFIX vann: <http://example.org/UNKNOWN/vann/>


linkml:String xsd:string

linkml:Integer xsd:integer

linkml:Boolean xsd:boolean

linkml:Float xsd:float

linkml:Double xsd:double

linkml:Decimal xsd:decimal

linkml:Time xsd:dateTime

linkml:Date xsd:date

linkml:Datetime xsd:dateTime

linkml:DateOrDatetime linkml:DateOrDatetime

linkml:Uriorcurie IRI

linkml:Curie xsd:string

linkml:Uri IRI

linkml:Ncname xsd:string

linkml:Objectidentifier IRI

linkml:Nodeidentifier NONLITERAL

<hhttp://terminology.tib.eu/schemaOntology> CLOSED {
    (  $<hhttp://terminology.tib.eu/schemaOntology_tes> (  &<hhttp://terminology.tib.eu/schemaRequiredMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaRequiredMetadata> ] ? ;
          &<hhttp://terminology.tib.eu/schemaRecommendedMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaRecommendedMetadata> ] ? ;
          &<hhttp://terminology.tib.eu/schemaOptionalMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaOptionalMetadata> ] ? ;
          dc:title @linkml:String ;
          vann:preferredNamespacePrefix @linkml:String ;
          <http://purl.obolibrary.org/obo/MOD_acronym> @linkml:String ? ;
          dc:license @linkml:Uriorcurie ;
          dc:alternative @linkml:String ? ;
          idot:alternatePrefix @linkml:String ?
       ) ;
       rdf:type [ owl:Ontology ]
    )
}

<hhttp://terminology.tib.eu/schemaOptionalMetadata> {
    (  $<hhttp://terminology.tib.eu/schemaOptionalMetadata_tes> rdf:type . * ;
       rdf:type [ <hhttp://terminology.tib.eu/schemaOptionalMetadata> ] ?
    )
}

<hhttp://terminology.tib.eu/schemaRecommendedMetadata> {
    (  $<hhttp://terminology.tib.eu/schemaRecommendedMetadata_tes> (  dc:alternative @linkml:String ? ;
          idot:alternatePrefix @linkml:String ?
       ) ;
       rdf:type [ <hhttp://terminology.tib.eu/schemaRecommendedMetadata> ] ?
    )
}

<hhttp://terminology.tib.eu/schemaRequiredMetadata> {
    (  $<hhttp://terminology.tib.eu/schemaRequiredMetadata_tes> (  dc:title @linkml:String ;
          vann:preferredNamespacePrefix @linkml:String ;
          <http://purl.obolibrary.org/obo/MOD_acronym> @linkml:String ? ;
          dc:license @linkml:Uriorcurie
       ) ;
       rdf:type [ <hhttp://terminology.tib.eu/schemaRequiredMetadata> ]
    )
}

<hhttp://terminology.tib.eu/schemaSkosConceptScheme> CLOSED {
    (  $<hhttp://terminology.tib.eu/schemaSkosConceptScheme_tes> (  &<hhttp://terminology.tib.eu/schemaRequiredMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaRequiredMetadata> ] ? ;
          &<hhttp://terminology.tib.eu/schemaRecommendedMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaRecommendedMetadata> ] ? ;
          &<hhttp://terminology.tib.eu/schemaOptionalMetadata_tes> ;
          rdf:type [ <hhttp://terminology.tib.eu/schemaOptionalMetadata> ] ? ;
          dc:title @linkml:String ;
          vann:preferredNamespacePrefix @linkml:String ;
          <http://purl.obolibrary.org/obo/MOD_acronym> @linkml:String ? ;
          dc:license @linkml:Uriorcurie ;
          dc:alternative @linkml:String ? ;
          idot:alternatePrefix @linkml:String ?
       ) ;
       rdf:type [ skos:ConceptScheme ]
    )
}


